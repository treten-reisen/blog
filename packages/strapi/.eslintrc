{
  "extends": [
    "eslint:recommended",
    "@strapi-community",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier", "import"],
  "env": {
    "node": true
  },
  "root": true,
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": { "order": "asc", "caseInsensitive": true }
      }
    ]
  }
}

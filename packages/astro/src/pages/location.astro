---
import Layout from "../layouts/Layout.astro"
import Seo from "../components/Seo.astro"
import { getGlobal } from "../data/get-global"
import OlMap from "../components/map/map"

const global = await getGlobal()

const strapiUrl = import.meta.env.STRAPI_API_URL
const apiKey = import.meta.env.GEOAPIFY_API_KEY
---

<Layout noPadding>
  <Seo type="website" global={global.data} slot="seo" />
  <OlMap backendUrl={strapiUrl} apiKey={apiKey} client:only="react" />
</Layout>

---
export type Props = {
  logoUrl: string
}

const { logoUrl } = Astro.props as Props
---

<header class="bg-gray-600">
  <div class="px-responsive py-4 md:container">
    <a href="/" tabindex="0" title="Zur Startseite">
      <img class="h-6 self-start" src={logoUrl} alt="Logo" />
    </a>
  </div>
</header>

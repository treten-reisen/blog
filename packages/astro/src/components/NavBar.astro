---
import Navigation from "./navigation/navigation"

export type Props = {
  logoUrl: string
}

const { logoUrl } = Astro.props as Props
---

<Navigation
  logoUrl={logoUrl}
  currentUrl={Astro.url.pathname}
  items={[
    {
      url: "/",
      label: "Home",
      exact: true,
    },
    {
      url: "/articles",
      label: "Blog",
    },
    {
      url: "/live-standort",
      label: "Karte",
    },
  ]}
  client:load
/>
